{
  "name": "dstash-core",
  "version": "0.0.1",
  "scripts": {
    "dev": "tsc && dotenv -e ../../.env -- nodemon -- src/app.ts --transpile-only",
    "migrate": "mikro-orm migration:create --path src/migrations",
    "format": "prettier --ignore-path ../../.gitignore --loglevel warn --write .",
    "lint": "cross-env TIMING=1 eslint --ignore-path ../../.gitignore --fix .",
    "build": "tsc"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "dependencies": {
    "@mikro-orm/better-sqlite": "^5.1.1",
    "@mikro-orm/core": "^5.1.1",
    "@mikro-orm/migrations": "^5.1.1",
    "@mikro-orm/reflection": "^5.1.1",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "4.6.2"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.1.1",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^5.0.0",
    "dstash-config": "workspace:^0.0.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unicorn": "^41.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0"
  }
}
